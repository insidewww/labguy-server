model Work {
    id                 Int            @id @default(autoincrement())
    general            GeneralSection @relation(fields: [generalSectionSlug], references: [slug])
    images             ImageRef[]
    videos             VideoRef[]
    medium             String[]
    dimensions         String
    year               Int
    generalSectionSlug String
    mediaReferenceId   Int
    imageRefEtag       String
    videoRefEtag       String
}

model GeneralSection {
    id Int @id @default(autoincrement())

    title       String
    description String?
    published   Boolean  @default(false)
    slug        String   @unique
    tags        String[] @unique

    createdAt DateTime  @default(now())
    updatedAt DateTime? @updatedAt

    Post     Post[]
    Work     Work[]
    ImageRef ImageRef[]
    VideoRef VideoRef[]
    Project  Project[]
}

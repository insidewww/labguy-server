model GeneralSection {
    id Int @id @default(autoincrement())

    title       String
    description String?
    published   Boolean @default(false)
    slug        String  @unique
    tags        Tag[]   @relation("GeneralTags")

    createdAt DateTime  @default(now())
    updatedAt DateTime? @updatedAt

    Project Project[]
    Work    Work[]
    Post    Post[]
}

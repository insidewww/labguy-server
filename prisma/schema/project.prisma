model Project {
    id         Int       @id @default(autoincrement())
    subtitle   String?
    start_date DateTime?
    end_date   DateTime?
    text       String?
    venue      String?
    urls       Json?

    images ImageRef[]
    videos VideoRef[]
    works  Work[]

    general   GeneralSection @relation(fields: [generalId], references: [id], onDelete: Cascade)
    generalId Int            @unique
}

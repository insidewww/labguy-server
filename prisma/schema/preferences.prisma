model Preferences {
    id                        Int       @id @default(autoincrement())
    brandname                 String    @unique
    domain                    String    @unique
    homepage_header           String
    homepage_subheader        String
    homepage_background_image ImageRef?
    homepage_background_video VideoRef?
    enable_dashboard_darkmode Boolean   @default(false)
    enable_portfolio_pdf      Boolean   @default(false)
    api_server_url            String
    api_youtube_key           String
    api_cld_url               String
    api_cld_key               String
    api_cld_cloudname         String
}

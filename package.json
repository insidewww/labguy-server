    "url": "https://github.com/jakubkanna/labguy-server/issues"
  },
  "homepage": "https://github.com/jakubkanna/labguy-server#readme",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "devDependencies": {
    "@eslint/js": "^9.7.0",
    "eslint": "^8.57.0",
    "globals": "^15.8.0",

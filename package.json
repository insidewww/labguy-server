{
  "name": "labguy-server",
  "private": true,
  "version": "1.0.0",
  "engines": {
    "node": ">=22.5.1"
  },
  "description": "Express REST Server featuring: JWT Authentication and Authorization, Prisma (posgresql db connection), Cloudinary and standard middlewares like Helmet; Limitter; Morgan",
  "main": "./bin/www",
  "scripts": {
    "prebuild": " ts-node scripts/postinstall && ts-node scripts/copy-assets",
    "build": "tsc",
    "start": "node ./bin/www",
    "dev": "nodemon ./app.ts",
    "postinstall": "npm run build && npx prisma db push && npx prisma db seed",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jakubkanna/labguy-server.git"
